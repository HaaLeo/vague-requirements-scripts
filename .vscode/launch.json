{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Slice corpus",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/slice_corpus.py",
            "console": "internalConsole"
        },
        {
            "name": "Format",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/format.py",
            "console": "internalConsole"
        },
        {
            "name": "Calculate metrics",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/calc_metrics.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Compare Label",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/compare.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Inter-rater reliability",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/calc_inter_rater_reliability.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
